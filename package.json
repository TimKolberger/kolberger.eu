    "blueimp-md5": "2.19.0",
    "clsx": "2.1.1",
    "isbot": "5.1.30",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.2"
  },
  "devDependencies": {
    "@react-router/dev": "7.8.2",
    "@react-router/fs-routes": "7.8.2",
    "@tailwindcss/vite": "4.1.12",
    "@types/blueimp-md5": "2.18.2",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2",
    "vite": "7.1.4"
  },
  "engines": {
    "node": ">=22.0.0"
