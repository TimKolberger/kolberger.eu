    "react-dom": "19.1.1",
    "react-router": "7.9.3"
  },
  "devDependencies": {
    "@react-router/dev": "7.9.3",
    "@react-router/fs-routes": "7.9.3",
    "@tailwindcss/vite": "4.1.14",
    "@types/blueimp-md5": "2.18.2",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
